@{
    ViewData["Title"] = "Home Page";

}
@section scripts {
    <script src="/js/search.js"></script>
    <script src="/js/bookmark.js"></script>
    <script src="/js/stats.js"></script>
}
<div class="container" id="main">
    <div class="col-sm-3" id="searchModule">

        <form id="search">
            <input type="text" id="searchBar" placeholder="Nom du projet" />
            <input type="submit" />
        </form>
        <a href="#" onclick="getBookmarks()">Mon projet</a>
        <div id="searchResult">
        </div>
    </div>
    <!--<div class="col-sm-9" id="stats">
    <div class="container">-->
    <div class="col-sm-6 graphModule">
        <div class="graph">
            <div id="tester" style="width:100%;height:350px;"></div>
        </div>
    </div>
    <div class="col-sm-3 graphModule">
        <div class="graph" id="contributors">
        </div>

    </div>
</div>

